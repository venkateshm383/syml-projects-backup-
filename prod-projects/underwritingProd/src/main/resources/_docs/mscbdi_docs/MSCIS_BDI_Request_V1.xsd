<?xml version="1.0" encoding="utf-8"?>
<xs:schema xmlns:b="http://schemas.microsoft.com/BizTalk/2003" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:r="http://MSC.IntegrationService.Schemas.MorWEB.BDI.Request.1" targetNamespace="http://MSC.IntegrationService.Schemas.MorWEB.BDI.Request.1" version="1.1.1">
	<xs:element name="BDIRequest">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="r:CommonData"/>
				<xs:element ref="r:CustomerData"/>
				<xs:element ref="r:MortgageApplication"/>
			</xs:sequence>
			<xs:attribute name="createdDateTime" type="r:typeDateTime" use="required"/>
			<xs:attribute name="createdUnitId" type="r:typeUnitId" use="required"/>
			<xs:attribute name="createdUserId" type="r:typeUserId" use="required"/>
		</xs:complexType>
		<xs:key name="keyAddress">
			<xs:selector xpath="r:CommonData/r:AddressList/*"/>
			<xs:field xpath="@key"/>
		</xs:key>
		<xs:key name="keyAddressCanada">
			<xs:selector xpath="r:CommonData/r:AddressList/r:ApplicationAddressCanada"/>
			<xs:field xpath="@key"/>
		</xs:key>
		<xs:key name="keyCustomer">
			<xs:selector xpath="r:CustomerData/r:CustomerList/*"/>
			<xs:field xpath="@key"/>
		</xs:key>
		<xs:key name="keyAsset">
			<xs:selector xpath="r:CommonData/r:AssetList/*"/>
			<xs:field xpath="@key"/>
		</xs:key>
		<xs:key name="keyAssetRealEstate">
			<xs:selector xpath="r:CommonData/r:AssetList/r:AssetRealEstate"/>
			<xs:field xpath="@key"/>
		</xs:key>
		<xs:key name="keyAssetOther">
			<xs:selector xpath="r:CommonData/r:AssetList/r:AssetOther"/>
			<xs:field xpath="@key"/>
		</xs:key>
		<xs:key name="keyLiability">
			<xs:selector xpath="r:CommonData/r:AssetList/*"/>
			<xs:field xpath="@key"/>
		</xs:key>
		<xs:key name="keyLiabilityRealEstate">
			<xs:selector xpath="r:CommonData/r:LiabilityList/r:LiabilityRealEstate"/>
			<xs:field xpath="@key"/>
		</xs:key>
		<xs:key name="keyLiabilityOther">
			<xs:selector xpath="r:CommonData/r:LiabilityList/r:LiabilityOther"/>
			<xs:field xpath="@key"/>
		</xs:key>
		<xs:key name="keyCreditBureau">
			<xs:selector xpath="r:CommonData/r:CreditBureauList/r:CreditBureau"/>
			<xs:field xpath="@key"/>
		</xs:key>
		<!--Customer Address must reference an Address-->
		<xs:keyref name="fkCustomerAddress_Address" refer="r:keyAddress">
			<xs:selector xpath="r:CustomerData/r:CustomerAddressList/*"/>
			<xs:field xpath="@refkeyAddress"/>
		</xs:keyref>
		<!--All customers associated with address must be from Customer list-->
		<xs:keyref name="fkCustomerAddress_Customer" refer="r:keyCustomer">
			<xs:selector xpath="r:CustomerData/r:CustomerAddressList/*/r:CustomerReference"/>
			<xs:field xpath="@refkeyCustomer"/>
		</xs:keyref>
		<!--Any address asset must be of type AssetRealEstate-->
		<xs:keyref name="fkCustomerAddress_Asset" refer="r:keyAssetRealEstate">
			<xs:selector xpath="r:CustomerData/r:CustomerAddressList/*/*"/>
			<xs:field xpath="@refkeyAsset"/>
		</xs:keyref>
		<!--All address liabilities (existing mortgages) must be of type LiabiilityRealEstate-->
		<xs:keyref name="fkCustomerAddress_Liability" refer="r:keyLiabilityRealEstate">
			<xs:selector xpath="r:CustomerData/r:CustomerAddressList/*/r:CustomerLiabilityRealEstate"/>
			<xs:field xpath="@refkeyLiability"/>
		</xs:keyref>
		<!-- Subject property must be canadian address -->
		<xs:keyref name="fkSubjectProperty_Address" refer="r:keyAddressCanada">
			<xs:selector xpath="r:MortgageApplication/r:SubjectPropertyList/r:SubjectProperty/r:SubjectPropertyAddress"/>
			<xs:field xpath="@refkeyAddress"/>
		</xs:keyref>
		<!--Other customer assets -->
		<xs:keyref name="fkCustomerAssetOther_AssetOther" refer="r:keyAssetOther">
			<xs:selector xpath="r:CustomerData/r:CustomerAssetList/r:CustomerAssetOther"/>
			<xs:field xpath="@refkeyAsset"/>
		</xs:keyref>
		<xs:keyref name="fkCustomerAssetOther_Customer" refer="r:keyCustomer">
			<xs:selector xpath="r:CustomerData/r:CustomerAssetList/*/r:CustomerReference"/>
			<xs:field xpath="@refkeyCustomer"/>
		</xs:keyref>
		<!--Other customer liabilities -->
		<xs:keyref name="fkCustomerLiabilityOther_LiabilityOther" refer="r:keyLiabilityOther">
			<xs:selector xpath="r:CustomerData/r:CustomerLiabilityList/r:CustomerLiabilityOther"/>
			<xs:field xpath="@refkeyLiability"/>
		</xs:keyref>
		<xs:keyref name="fkCustomerLiabilityOther_Customer" refer="r:keyCustomer">
			<xs:selector xpath="r:CustomerData/r:CustomerLiabilityList/*/r:CustomerReference"/>
			<xs:field xpath="@refkeyCustomer"/>
		</xs:keyref>
		<!--Other customer Credit Bureau -->
		<xs:keyref name="fkCustomerCreditBureau_CreditBureau" refer="r:keyCreditBureau">
			<xs:selector xpath="r:CustomerData/r:CustomerCreditBureauList/r:CustomerCreditBureau"/>
			<xs:field xpath="@refkeyCreditBureau"/>
		</xs:keyref>
		<xs:keyref name="fkCustomerCreditBureau_Customer" refer="r:keyCustomer">
			<xs:selector xpath="r:CustomerData/r:CustomerCreditBureauList/*/r:CustomerReference"/>
			<xs:field xpath="@refkeyCustomer"/>
		</xs:keyref>
	</xs:element>
	<xs:element name="AddressCanada" type="r:typeAddressCanada"/>
	<xs:element name="AddressUSA" type="r:typeAddressUSA"/>
	<xs:element name="AddressOther" type="r:typeAddressOther"/>
	<xs:element name="AddressList">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="r:ApplicationAddress" minOccurs="0" maxOccurs="99999"/>
			</xs:sequence>
		</xs:complexType>
		<xs:unique name="uixApplicationAddress">
			<xs:selector xpath="*"/>
			<xs:field xpath="@cityTown"/>
			<xs:field xpath="@provinceCode"/>
			<xs:field xpath="@postalCode"/>
			<xs:field xpath="@countryCode"/>
			<xs:field xpath="*/@POBoxRRNumber"/>
			<xs:field xpath="*/@streetName"/>
			<xs:field xpath="*/@streetNumber"/>
			<xs:field xpath="*/@unitNumber"/>
			<xs:field xpath="*/@streetType"/>
			<xs:field xpath="*/@streetDirection"/>
		</xs:unique>
	</xs:element>
	<xs:element name="AddressOccupancy" type="r:typeAddressOccupancy" abstract="true"/>
	<xs:element name="AddressOccupancyNone" type="r:typeAddressOccupancyNone" substitutionGroup="r:AddressOccupancy"/>
	<xs:element name="AddressOccupancyOwnerOccupied" type="r:typeAddressOccupancyOwnerOccupied" substitutionGroup="r:AddressOccupancy"/>
	<xs:element name="AddressOccupancyPartialOwnerOccupied" type="r:typeAddressOccupancyPartialOwnerOccupied" substitutionGroup="r:AddressOccupancy"/>
	<xs:element name="AddressOccupancyRental" type="r:typeAddressOccupancyRental" substitutionGroup="r:AddressOccupancy"/>
	<xs:element name="AddressOccupancyTenant" type="r:typeAddressOccupancyTenant" substitutionGroup="r:AddressOccupancy"/>
	<xs:element name="ApplicationNote">
		<xs:complexType>
			<xs:attribute name="noteType" type="r:typeApplicationNoteType"/>
			<xs:attribute name="noteDateTime" type="r:typeDateTime"/>
			<xs:attribute name="noteText" type="r:typeStringNonEmpty"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="ApplicationNoteList">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="r:ApplicationNote" minOccurs="0" maxOccurs="99999"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="ApplicationAddress" type="r:typeApplicationAddress" abstract="true"/>
	<xs:element name="ApplicationAddressCanada" type="r:typeApplicationAddressCanada" substitutionGroup="r:ApplicationAddress"/>
	<xs:element name="ApplicationAddressUSA" type="r:typeApplicationAddressUSA" substitutionGroup="r:ApplicationAddress"/>
	<xs:element name="ApplicationAddressOther" type="r:typeApplicationAddressOther" substitutionGroup="r:ApplicationAddress"/>
	<xs:element name="AssetList">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="r:Asset" minOccurs="0" maxOccurs="99999"/>
			</xs:sequence>
		</xs:complexType>
		<xs:unique name="uidAssetRealEstate_Address">
			<xs:selector xpath="r:AssetRealEstate"/>
			<xs:field xpath="@refkeyAddress"/>
		</xs:unique>
	</xs:element>
	<xs:element name="Asset" type="r:typeAsset" abstract="true"/>
	<xs:element name="AssetRealEstate" type="r:typeAssetRealEstate" substitutionGroup="r:Asset"/>
	<xs:element name="AssetOther" type="r:typeAssetOther" substitutionGroup="r:Asset"/>
	<xs:element name="CreditBureauList">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="r:CreditBureau" minOccurs="0" maxOccurs="99999"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="CreditBureau" type="r:typeCreditBureau"/>
	<xs:element name="CreditReport" type="r:typeCreditReport" abstract="true"/>
	<xs:element name="CreditReportEquifax" type="r:typeCreditReportEquifax" substitutionGroup="r:CreditReport"/>
	<xs:element name="BankruptcyHistory">
		<xs:complexType>
			<xs:attribute name="debtAmount">
				<xs:simpleType>
					<xs:restriction base="xs:decimal">
						<xs:minInclusive value="0"/>
						<xs:maxInclusive value="999999.99"/>
						<xs:fractionDigits value="2"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="dateDeclared" type="r:typeDate"/>
			<xs:attribute name="dateDischarged" type="r:typeDate"/>
			<xs:attribute name="monthlyRepayment">
				<xs:simpleType>
					<xs:restriction base="xs:decimal">
						<xs:minInclusive value="0"/>
						<xs:maxInclusive value="999999.99"/>
						<xs:fractionDigits value="2"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="otherDetails">
				<xs:simpleType>
					<xs:restriction base="r:typeStringNonEmpty">
						<xs:maxLength value="255"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="BankruptcyHistoryList">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="r:BankruptcyHistory" minOccurs="0" maxOccurs="99999"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="CommonData">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="r:AssetList"/>
				<xs:element ref="r:LiabilityList"/>
				<xs:element ref="r:AddressList"/>
				<xs:element ref="r:CreditBureauList" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="Condo">
		<xs:complexType>
			<xs:attribute name="annualCondoFees">
				<xs:simpleType>
					<xs:restriction base="xs:decimal">
						<xs:maxInclusive value="9999999999"/>
						<xs:minInclusive value="0"/>
						<xs:fractionDigits value="2"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="ContactTelephoneNumber">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="r:TelephoneNumber"/>
			</xs:sequence>
			<xs:attribute name="usage" type="r:typeContactTelephoneUsageType"/>
			<xs:attribute name="contactTime">
				<xs:simpleType>
					<xs:restriction base="r:typeStringNonEmpty">
						<xs:maxLength value="30"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="CustomerData">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="r:CustomerList"/>
				<xs:element ref="r:CustomerAddressList"/>
				<xs:element ref="r:CustomerAssetList"/>
				<xs:element ref="r:CustomerLiabilityList"/>
				<xs:element ref="r:CustomerCreditBureauList" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="CustomerList">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="r:CustomerBorrower"/>
				<xs:element ref="r:Customer" minOccurs="0" maxOccurs="4"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="Customer" type="r:typeCustomer" abstract="true"/>
	<xs:element name="CustomerBorrower" type="r:typeCustomer" substitutionGroup="r:Customer"/>
	<xs:element name="CustomerGuarantor" type="r:typeCustomer" substitutionGroup="r:Customer"/>
	<xs:element name="CustomerAddressList" type="r:typeCustomerAddressList">
		<!--Address must only be used once-->
		<xs:unique name="uixCustomerAddressList_Address">
			<xs:selector xpath="*"/>
			<xs:field xpath="@refkeyAddress"/>
		</xs:unique>
		<!--Address Assets must only be used once-->
		<xs:unique name="uixCustomerAddressList_Asset">
			<xs:selector xpath="*/*"/>
			<xs:field xpath="@refkeyAsset"/>
		</xs:unique>
		<!--Address liabilities must only be used once-->
		<xs:unique name="uixCustomerAddressList_Liability">
			<xs:selector xpath="*/*"/>
			<xs:field xpath="@refkeyLiability"/>
		</xs:unique>
	</xs:element>
	<xs:element name="CustomerAddress" type="r:typeCustomerAddress" abstract="true"/>
	<xs:element name="CustomerAddressPreviousResidence" substitutionGroup="r:CustomerAddress">
		<xs:complexType>
			<xs:complexContent>
				<xs:restriction base="r:typeCustomerAddress">
					<xs:sequence>
						<xs:element ref="r:CustomerReference" maxOccurs="unbounded"/>
						<xs:element ref="r:CustomerLiabilityRealEstate" minOccurs="0" maxOccurs="99999"/>
						<xs:choice>
							<xs:element ref="r:AddressOccupancyOwnerOccupied"/>
							<xs:element ref="r:AddressOccupancyPartialOwnerOccupied"/>
							<xs:element ref="r:AddressOccupancyRental"/>
							<xs:element ref="r:AddressOccupancyTenant"/>
						</xs:choice>
					</xs:sequence>
					<xs:attribute name="fromDate" type="r:typeDate" use="required"/>
					<xs:attribute name="toDate" type="r:typeDate" use="required"/>
				</xs:restriction>
			</xs:complexContent>
		</xs:complexType>
		<xs:key name="keyCustomerAddressPreviouseResidence_Customer">
			<xs:selector xpath="r:CustomerReference"/>
			<xs:field xpath="@refkeyCustomer"/>
		</xs:key>
		<xs:keyref name="fkCustomerAddressPreviousResidence_ExistingMortgage_Customer" refer="r:keyCustomerAddressPreviouseResidence_Customer">
			<xs:selector xpath="r:CustomerLiabilityRealEstate/r:CustomerReference"/>
			<xs:field xpath="@refkeyCustomer"/>
		</xs:keyref>
	</xs:element>
	<xs:element name="CustomerAddressPrimaryResidence" substitutionGroup="r:CustomerAddress">
		<xs:complexType>
			<xs:complexContent>
				<xs:restriction base="r:typeCustomerAddress">
					<xs:sequence>
						<xs:element ref="r:CustomerReference" maxOccurs="unbounded"/>
						<xs:element ref="r:CustomerLiabilityRealEstate" minOccurs="0" maxOccurs="99999"/>
						<xs:choice>
							<xs:element ref="r:AddressOccupancyOwnerOccupied"/>
							<xs:element ref="r:AddressOccupancyPartialOwnerOccupied"/>
							<xs:element ref="r:AddressOccupancyRental"/>
							<xs:element ref="r:AddressOccupancyTenant"/>
						</xs:choice>
					</xs:sequence>
					<xs:attribute name="fromDate" type="r:typeDate" use="required"/>
					<xs:attribute name="toDate" type="r:typeDate" use="prohibited"/>
				</xs:restriction>
			</xs:complexContent>
		</xs:complexType>
		<xs:key name="keyCustomerAddressPrimaryResidence_Customer">
			<xs:selector xpath="r:CustomerReference"/>
			<xs:field xpath="@refkeyCustomer"/>
		</xs:key>
		<xs:keyref name="fkCustomerAddressPrimaryResidence_ExistingMortgage_Customer" refer="r:keyCustomerAddressPrimaryResidence_Customer">
			<xs:selector xpath="r:CustomerLiabilityRealEstate/r:CustomerReference"/>
			<xs:field xpath="@refkeyCustomer"/>
		</xs:keyref>
	</xs:element>
	<xs:element name="CustomerAddressSecondaryResidence" substitutionGroup="r:CustomerAddress">
		<xs:complexType>
			<xs:complexContent>
				<xs:restriction base="r:typeCustomerAddress">
					<xs:sequence>
						<xs:element ref="r:CustomerReference" maxOccurs="unbounded"/>
						<xs:element ref="r:CustomerLiabilityRealEstate" minOccurs="0" maxOccurs="99999"/>
						<xs:choice>
							<xs:element ref="r:AddressOccupancyOwnerOccupied"/>
							<xs:element ref="r:AddressOccupancyPartialOwnerOccupied"/>
							<xs:element ref="r:AddressOccupancyRental"/>
							<xs:element ref="r:AddressOccupancyTenant"/>
						</xs:choice>
					</xs:sequence>
					<xs:attribute name="fromDate" type="r:typeDate" use="required"/>
					<xs:attribute name="toDate" type="r:typeDate" use="prohibited"/>
				</xs:restriction>
			</xs:complexContent>
		</xs:complexType>
		<xs:key name="keyCustomerAddressSecondaryResidence_Customer">
			<xs:selector xpath="r:CustomerReference"/>
			<xs:field xpath="@refkeyCustomer"/>
		</xs:key>
		<xs:keyref name="fkCustomerAddressSecondaryResidence_ExistingMortgage_Customer" refer="r:keyCustomerAddressSecondaryResidence_Customer">
			<xs:selector xpath="r:CustomerLiabilityRealEstate/r:CustomerReference"/>
			<xs:field xpath="@refkeyCustomer"/>
		</xs:keyref>
	</xs:element>
	<xs:element name="CustomerAddressVacationProperty" substitutionGroup="r:CustomerAddress">
		<xs:complexType>
			<xs:complexContent>
				<xs:restriction base="r:typeCustomerAddress">
					<xs:sequence>
						<xs:element ref="r:CustomerReference" maxOccurs="unbounded"/>
						<xs:element ref="r:CustomerLiabilityRealEstate" minOccurs="0" maxOccurs="99999"/>
						<xs:choice>
							<xs:element ref="r:AddressOccupancyOwnerOccupied"/>
							<xs:element ref="r:AddressOccupancyPartialOwnerOccupied"/>
							<xs:element ref="r:AddressOccupancyRental"/>
						</xs:choice>
					</xs:sequence>
					<xs:attribute name="fromDate" type="r:typeDate" use="required"/>
					<xs:attribute name="toDate" type="r:typeDate" use="prohibited"/>
				</xs:restriction>
			</xs:complexContent>
		</xs:complexType>
		<xs:key name="keyCustomerAddressVacationProperty_Customer">
			<xs:selector xpath="r:CustomerReference"/>
			<xs:field xpath="@refkeyCustomer"/>
		</xs:key>
		<xs:keyref name="fkCustomerAddressVacationProperty_ExistingMortgage_Customer" refer="r:keyCustomerAddressVacationProperty_Customer">
			<xs:selector xpath="r:CustomerLiabilityRealEstate/r:CustomerReference"/>
			<xs:field xpath="@refkeyCustomer"/>
		</xs:keyref>
	</xs:element>
	<xs:element name="CustomerAddressInvestmentProperty" substitutionGroup="r:CustomerAddress">
		<xs:complexType>
			<xs:complexContent>
				<xs:restriction base="r:typeCustomerAddress">
					<xs:sequence>
						<xs:element ref="r:CustomerReference" maxOccurs="unbounded"/>
						<xs:element ref="r:CustomerLiabilityRealEstate" minOccurs="0" maxOccurs="99999"/>
						<xs:choice>
							<xs:element ref="r:AddressOccupancyOwnerOccupied"/>
							<xs:element ref="r:AddressOccupancyPartialOwnerOccupied"/>
							<xs:element ref="r:AddressOccupancyRental"/>
						</xs:choice>
					</xs:sequence>
					<xs:attribute name="fromDate" type="r:typeDate" use="required"/>
					<xs:attribute name="toDate" type="r:typeDate" use="prohibited"/>
				</xs:restriction>
			</xs:complexContent>
		</xs:complexType>
		<xs:key name="keyCustomerAddressInvestmentProperty_Customer">
			<xs:selector xpath="r:CustomerReference"/>
			<xs:field xpath="@refkeyCustomer"/>
		</xs:key>
		<xs:keyref name="fkCustomerAddressInvestmentProperty_ExistingMortgage_Customer" refer="r:keyCustomerAddressInvestmentProperty_Customer">
			<xs:selector xpath="r:CustomerLiabilityRealEstate/r:CustomerReference"/>
			<xs:field xpath="@refkeyCustomer"/>
		</xs:keyref>
	</xs:element>
	<xs:element name="CustomerAddressCorrespondence" substitutionGroup="r:CustomerAddress">
		<xs:complexType>
			<xs:complexContent>
				<xs:restriction base="r:typeCustomerAddress">
					<xs:sequence>
						<xs:element ref="r:CustomerReference" maxOccurs="unbounded"/>
						<xs:element ref="r:CustomerLiabilityRealEstate" minOccurs="0" maxOccurs="99999"/>
						<xs:choice>
							<xs:element ref="r:AddressOccupancyNone"/>
							<xs:element ref="r:AddressOccupancyTenant"/>
						</xs:choice>
					</xs:sequence>
					<xs:attribute name="fromDate" type="r:typeDate" use="required"/>
					<xs:attribute name="toDate" type="r:typeDate" use="prohibited"/>
				</xs:restriction>
			</xs:complexContent>
		</xs:complexType>
		<xs:key name="keyCustomerAddressCorrespondence_Customer">
			<xs:selector xpath="r:CustomerReference"/>
			<xs:field xpath="@refkeyCustomer"/>
		</xs:key>
		<xs:keyref name="fkCustomerAddressCorrespondence_ExistingMortgage_Customer" refer="r:keyCustomerAddressCorrespondence_Customer">
			<xs:selector xpath="r:CustomerLiabilityRealEstate/r:CustomerReference"/>
			<xs:field xpath="@refkeyCustomer"/>
		</xs:keyref>
	</xs:element>
	<xs:element name="CustomerAssetList">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="r:CustomerAssetOther" minOccurs="0" maxOccurs="99999"/>
			</xs:sequence>
		</xs:complexType>
		<!-- Assets must only be referenced once -->
		<xs:unique name="uixCustomerAsset_Asset">
			<xs:selector xpath="*"/>
			<xs:field xpath="@refkeyAsset"/>
		</xs:unique>
	</xs:element>
	<xs:element name="CustomerAssetOther">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="r:CustomerReference"/>
			</xs:sequence>
			<xs:attribute name="refkeyAsset" type="r:typeReferenceKey" use="required"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="CustomerLiabilityList">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="r:CustomerLiabilityOther" minOccurs="0" maxOccurs="99999"/>
			</xs:sequence>
		</xs:complexType>
		<!-- Other Liabilities must only be referenced once -->
		<xs:unique name="uixCustomerLiability_Liability">
			<xs:selector xpath="*"/>
			<xs:field xpath="@refkeyLiability"/>
		</xs:unique>
	</xs:element>
	<xs:element name="CustomerLiabilityOther">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="r:CustomerReference"/>
			</xs:sequence>
			<xs:attribute name="refkeyLiability" type="r:typeReferenceKey" use="required"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="CustomerLiabilityRealEstate">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="r:CustomerReference"/>
			</xs:sequence>
			<xs:attribute name="refkeyLiability" type="r:typeReferenceKey" use="required"/>
		</xs:complexType>
		<xs:unique name="uixCustomerLiability_Customer">
			<xs:selector xpath="r:CustomerReference"/>
			<xs:field xpath="@refkeyCustomer"/>
		</xs:unique>
	</xs:element>
	<xs:element name="CustomerCreditBureauList">
		<xs:complexType>
			<xs:sequence>
