	<xs:element name="MortgageApplication">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="r:SubjectPropertyList" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Currently only 1 subject property can be included for MorWEB applications. If there are multiple securing properties, enter the primary property within the SubjectPropertyList and create one or more Application notes to contain the details of the other properties.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="r:ApplicationNoteList" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Notes to be attached to the Application. Will show within MemoPad on Morweb</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="r:LoanList" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Currently only 1 mortgage loan can be ingested via MorWEB BDI (Broker Data Ingestion).</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="r:DownpaymentList" minOccurs="0"/>
			</xs:sequence>
			<xs:attribute name="isPreapproval" type="xs:boolean"/>
			<xs:attribute name="fundingDate" type="r:typeDate"/>
			<xs:attribute name="loanPurposeType" type="r:typeLoanPurposeType"/>
			<xs:attribute name="purchasePriceEstimatedValue" type="xs:float"/>
			<xs:attribute name="brokerUserId" type="r:typeUserId" use="optional"/>
			<xs:attribute name="agentUserId" type="r:typeUserId" use="optional"/>
			<xs:attribute name="adminUserId" type="r:typeUserId" use="optional"/>
		</xs:complexType>
